   "source": [
    "import pyttsx3\n",
    "import PyPDF2\n",
    "book = open('The_Da_Vinci_Code.pdf', 'rb')\n",
    "pdfReader = PyPDF2.PdfFileReader(book)\n",
    "speaker = pyttsx3.init()\n",
    "page = pdfReader.getPage(4)\n",
    "text = page.extractText()\n",
    "speaker.say(text)\n",
    "speaker.runAndWait()"
   ]
  }
